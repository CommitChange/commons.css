{
  "name": "@commitchange/commons.css",
  "version": "0.0.5",
  "description": "Normalize.css + utils + core UI components",
  "style": "lib/index.css",
  "scripts": {
    "build-lib-css": "postcss -c postcss-lib.js -l",
    "watch-example-css": "postcss -c postcss-example.js -w -l",
    "build-example": "mkdir -p example/dist && cp example/build.css example/dist/build.css && cp example/index.html  example/dist/index.html && browserify example/index.js -o example/dist/index.js -t [babelify --presets es2015]",
    "gh-pages": "git subtree push --prefix example/dist origin gh-pages",
    "watch-js": "budo example/index.js --dir example  --live -- -t [babelify --presets es2015]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CommitChange/styles.git"
  },
  "author": "yutakahoulette",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CommitChange/styles/issues"
  },
  "homepage": "https://github.com/CommitChange/styles#readme",
  "devDependencies": {
    "@commitchange/color-variables": "0.0.1",
    "@commitchange/grey-scale-variables": "0.0.1",
    "autoprefixer": "^6.5.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.2.1",
    "normalize.css": "^5.0.0",
    "postcss": "^5.2.4",
    "postcss-cli": "^2.6.0",
    "postcss-color-function": "^2.0.1",
    "postcss-import": "^8.1.2",
    "precss": "^1.4.0"
  },
  "dependencies": {
    "flimflam-render": "^0.1.4",
    "flyd": "^0.2.2",
    "highlight.js": "^9.7.0",
    "ramda": "^0.22.1",
    "snabbdom": "^0.5.3"
  }
}
